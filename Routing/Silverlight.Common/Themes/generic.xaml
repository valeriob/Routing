                                <Grid Width="20" Height="20"  x:Name="resizeAncor" VerticalAlignment="Bottom" HorizontalAlignment="Right" Opacity="0.5" >
                                    <Grid.Background>
                                        <LinearGradientBrush EndPoint="0,0" StartPoint="1,1">
                                            <GradientStop Color="#FF939393" Offset="0.025"  />
                                            <GradientStop Color="Transparent" Offset="0.6"/>
                                        </LinearGradientBrush>
                                    </Grid.Background>
                                    <Line X1="2" Y1="18" X2="18" Y2="2"  Stroke="Gray" StrokeThickness="1" />
                                    <Line X1="7" Y1="18" X2="18" Y2="7"   Stroke="#FF6E6E6E" StrokeThickness="1" />
                                    <Line X1="12" Y1="18" X2="18" Y2="12"  Stroke="#FF5C5A5A" StrokeThickness="1" />
                                </Grid>
                                
                                <!--<Image Name="resizeImage" Source="/Silverlight.Common;component/Assets/Images/resizecue.png" Width="18" Height="18" 
                                       VerticalAlignment="Bottom" HorizontalAlignment="Right" Cursor="SizeNWSE" />-->
                            </Grid>
                        </Border>
                        
                    </Grid>
  
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

